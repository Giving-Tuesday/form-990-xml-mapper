	</xsd:complexType>

	<!-- Type used for Part IV, Line 5 Post2017NOLCarryoverGrp -->
	<xsd:complexType name="Post2017NOLCarryoverGrpType">
		<xsd:sequence>

			<!-- Activity code -->
			<xsd:element name="PrincipalBusinessActivityCd" type="BusinessActivityCodeType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Activity code</Description>
						<LineNumber>Part IV, Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Available post-2017 NOL carryover -->
			<xsd:element name="AvlblPost2017NOLCarryoverAmt" type="USAmountType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Available post-2017 NOL carryover</Description>
						<LineNumber>Part IV, Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>

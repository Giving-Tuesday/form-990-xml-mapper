						<Description>Interest Acquired Description</Description>
						<LineNumber>Part II, Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Special Gain Code -->
			<xsd:element name="SpecialGainCd" minOccurs="0">

						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Short-Term Deferred Gain Remaining Amount -->
			<xsd:element name="ShortTermDefrdGainRmngAmt" type="USAmountPosType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Short-Term Deferred Gain Remaining Amount</Description>
						<LineNumber>Part II, Column (e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Long-Term Deferred Gain Remaining Amount -->

				<xsd:annotation>
					<xsd:documentation>
						<Description>Long-Term Deferred Gain Remaining Amount</Description>
						<LineNumber>Part II, Column (f)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Part III Table, Columns (a) - (f) -->
	<xsd:complexType name="InclsnEvtOthTrnsfrDurCurrTYGrpType">
		<xsd:sequence>

			<!-- EIN -->
			<xsd:element name="EIN" type="EINType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>EIN</Description>
						<LineNumber>Part III, Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Event Date -->
			<xsd:element name="EventDt" type="DateType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Event Date</Description>
						<LineNumber>Part III, Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Event Description -->
			<xsd:element name="EventDesc" type="LineExplanationType">
				<xsd:annotation>

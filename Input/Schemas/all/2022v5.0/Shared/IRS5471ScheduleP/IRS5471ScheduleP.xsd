			<xsd:element name="ReclassifiedSect959c2EPAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amounts Reclassified to section 959(c)(2) E&amp;P from section 959(c)(3) E&amp;P</Description>
						<LineNumber>7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>

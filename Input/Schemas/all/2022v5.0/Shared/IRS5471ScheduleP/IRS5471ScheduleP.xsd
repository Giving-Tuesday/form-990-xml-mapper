						<LineNumber>9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Amounts included as earnings invested in U.S. property and reclassified to section 959(c)(1) E&P -->
			<xsd:element name="EarnInvstUSPropReclassifiedAmt" type="USAmountType" minOccurs="0">
